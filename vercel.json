{
    "version": 2,
    "builds": [
        {
            "src": "GDG_project/wsgi.py",
            "use": "@vercel/python",
            "config": { "runtime": "python3.9" }
        },
        {
            "src": "vercel_build.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "staticfiles_build"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "GDG_project/wsgi.py"
        }
    ]
}