{
  "version": 2,
  "builds": [
    {
      "src": "GDG_project/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "buildCommand": "./vercel_build.sh",
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "GDG_project/wsgi.py"
    }
  ]
}
